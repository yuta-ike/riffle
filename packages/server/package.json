{
  "name": "server",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev --require dotenv/config --rs ./src/index.ts",
    "build": "tsc -p .",
    "start": "node --require dotenv/config ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext \".js,.ts\" --cache ./src",
    "type-check": "tsc -p . --noEmit",
    "format": "prettier --check \"**/*.{js,ts,json}\" .",
    "fix": "run-p lint:fix format:fix",
    "lint:fix": "yarn lint --fix",
    "format:fix": "yarn format --write",
    "prisma:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "resetdb:dev": "prisma migrate reset",
    "migrate:prod": "prisma migrate deploy",
    "studio": "prisma studio",
    "heroku-prebuild": "yarn migrate:prod"
  },
  "dependencies": {
    "@prisma/client": "3.4.0",
    "axios": "^0.24.0",
    "fastify": "^3.22.1",
    "fastify-cors": "^6.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "dotenv": "^10.0.0",
    "firebase-admin": "^10.0.0",
    "prisma": "3.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
